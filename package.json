{
  "name": "final-project",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.com/afaraone/final-project.svg?branch=master)](https://travis-ci.com/afaraone/final-project)",
  "main": "index.js",
  "scripts": {
    "backend": "cd lifestyle-app-backend && rails s",
    "frontend": "cd lifestyle-app-frontend && npm start",
    "go": "concurrently --kill-others \"npm:backend\" \"npm:frontend\"",
    "build": "cd lifestyle-app-frontend && npm install && npm run build && cd ..",
    "deploy-front": "cp -a lifestyle-app-frontend/build/. lifestyle-app-backend/public/",
    "deploy-back": "cd lifestyle-app-backend && bundle",
    "postinstall": "npm run build && npm run deploy && echo 'Frontend built!' && npm run deploy-back"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afaraone/final-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/afaraone/final-project/issues"
  },
  "homepage": "https://github.com/afaraone/final-project#readme",
  "dependencies": {
    "concurrently": "^4.0.1"
  }
}
